				<img src="/images/{{this.image}}" alt="IMG-BANNER">
				<a style="text-decoration:none" href="/Category?id={{this._id}}"
					class="block1-txt ab-t-l s-full flex-col-l-sb p-lr-38 p-tb-34 trans-03 respon3">
					<div class="block1-txt-child1 flex-col-l">
						<span style="color: #fa4251; text-decoration:none" class="block1-name ltext-102 trans-04 p-b-8">
							{{this.name}}
							
						</span>

						<span class="block1-info stext-102 trans-04">
